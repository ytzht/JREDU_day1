<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff9f9f9"
    tools:context="com.example.jredu.MainActivity_grzl">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="#f18302"
        android:id="@+id/relativeLayout2">
        <ImageView
            android:id="@id/left_icon"
            android:paddingRight="@dimen/mainpage_actionbar_leftbtn_rightpadding"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/videoplayer_back"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:contentDescription="@null"
            android:layout_marginLeft="5dp"/>
        <TextView android:id="@id/title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="个人资料"
            style="@style/MainPageTitleStyle"
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:id="@+id/relativeLayout3"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout2"
        android:layout_alignParentStart="true"
        android:gravity="center_horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:text="头像"
            android:layout_marginLeft="20dp"
            android:gravity="center"
            />
        <com.util.RoundImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/face_grzl"
            android:layout_marginEnd="30dp"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"/>
        <!--<ImageView-->
            <!--android:layout_width="60dp"-->
            <!--android:layout_height="60dp"-->
            <!--android:src="@drawable/videoplayer_back"-->
            <!--android:layout_centerVertical="true"-->
            <!--android:layout_alignParentEnd="true" />-->
        </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#ff000000"
        android:layout_below="@+id/relativeLayout3"
        android:id="@+id/fengexian"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout4"
        android:layout_below="@+id/fengexian">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:text="昵称"
            android:layout_marginLeft="20dp"

            android:gravity="center"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:id="@+id/nickname"
            android:gravity="center"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageView1" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/next"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/imageView1" />

        </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout5"
        android:layout_below="@+id/relativeLayout4">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:text="性别"
            android:layout_marginLeft="20dp"

            android:gravity="center"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:id="@+id/sexId"
            android:gravity="center"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageView2" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/next"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/imageView2" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout6"
        android:layout_below="@+id/relativeLayout5">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:text="级别"
            android:layout_marginLeft="20dp"
            android:gravity="center"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:id="@+id/level"
            android:gravity="center"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageView3" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/next"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/imageView3" />

    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.1dp"
        android:background="#ff000000"
        android:layout_below="@+id/relativeLayout6"
        android:id="@+id/fengexian2"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout7"
        android:layout_below="@+id/fengexian2">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:text="密码修改"
            android:layout_marginLeft="20dp"
            android:gravity="center"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:id="@+id/change_pwd"
            android:gravity="center"
            android:textSize="18sp"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/imageView4" />
        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/next"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/imageView4" />

    </RelativeLayout>




</RelativeLayout>
